&usb_host0_xhci {
#ifdef USBOTG_HOST
	dr_mode = "host";
#else
	usb-role-switch;
#endif

	port {
		typec_role_sw: endpoint {
			/* Role Switch */

			remote-endpoint = <&fusb_role_sw>;
		};
	};

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			/* HS */
			reg = <0>;

			typec_hs: endpoint {
				remote-endpoint = <&fusb_hs>;
			};
		};

		port@1 {
			/* SS */
			reg = <1>;

			typec_ss: endpoint {
				remote-endpoint = <&fusb_ss>;
			};
		};
	};
};

